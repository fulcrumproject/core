      "email": "participant11@fulcrumproject.org",
      "emailVerified": true,
      "firstName": "Fulcrum",
      "lastName": "Participant 1",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": ["participant"],
      "attributes": {
        "participant_id": "participant1-id"
      }
