  "clientScopes": [
    {
      "name": "fulcrum_ids",
      "description": "User attribute mappers for participant_id and agent_id",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": true,
        "display.on.consent.screen": false,
